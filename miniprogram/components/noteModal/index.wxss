@import "/colorui/main.wxss";
@import "/colorui/icon.wxss";

.modal-btns {
  border-top: 2rpx solid #ccc;
  display: flex;
}

.modal-btns .item-btn {
  width: 50%;
  height: 100rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
}

.modal-btns  .btn-confirm {
  color: #39b54a;
  border-left: 2rpx solid #ccc;
}